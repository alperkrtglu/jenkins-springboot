@Library('jte-example') _

pipeline {
  agent any

  stages {
    stage('Checkout') {
      steps {
       checkout scm
      }
    }

    stage('Hello') {
      steps {
        helloProject()
      }
    }

    stage('Clean') {
      steps {
        cleanProject()
      }
    }

    stage('Build') {
      steps {
        buildProject()
      }
    }

  }
}