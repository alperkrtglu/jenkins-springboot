@Library('jte-example') _

pipeline {
  agent any

  triggers {
    cron('* * * * *')
  }
  stages {
    stage('Checkout') {
      steps {
       checkout scm
      }
    }

    stage('Hello') {
      steps {
        helloProject('Alper')
      }
    }

    stage('Clean') {
      steps {
        cleanProject()
      }
    }

    stage('Build') {
      steps {
        buildProject()
      }
    }

    stage('Cronjob') {
      steps {
        triggerProject()
      }
    }

  }
}